{
  "name": "ship",
  "version": "0.0.1",
  "description": "Wordpress Theme framework",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dist": "NODE_ENV=production MIX_BUILD_DIST=true node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "engines": {
    "npm": ">=2.1.8"
  },
  "keywords": [
    "wordpress",
    "theme",
    "framework",
    "bootstrap",
    "underscores",
    "understrap"
  ],
  "author": "Aaron Collegeman",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/withfatpanda/ship/issues"
  },
  "homepage": "https://github.com/withfatpanda/ship",
  "devDependencies": {
    "del": "^3.0.0",
    "font-awesome": "^4.7.0",
    "bootstrap": "4.0.0-beta",
    "vue": "^2.5.13",
    "bootstrap-vue": "^1.4.0",
    "laravel-mix": "^1.7.2"
  }
}
